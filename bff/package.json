    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "deploy": "npm run build && npm run start",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix ../marvel_poc_ui && npm run build --prefix ../marvel_poc_ui"
  },
